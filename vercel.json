{
  "version": 2,
  "builds": [
    { "src": "api/webhook.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/webhook.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
